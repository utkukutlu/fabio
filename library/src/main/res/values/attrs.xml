<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Fabio">
        <attr name="fbExpandMode" format="enum">
            <enum name="top" value="0"/>
            <enum name="right" value="1"/>
            <enum name="bottom" value="2"/>
            <enum name="left" value="3"/>
        </attr>
        <attr name="fbOpenColor" format="color"/>
        <attr name="fbCloseColor" format="color"/>
        <attr name="fbOpenIcon" format="reference"/>
        <attr name="fbCloseIcon" format="reference"/>
        <attr name="fbOverlayLayout" format="reference"/>
        <attr name="fbOverlayColor" format="color"/>
        <attr name="fbAnimationDuration" format="integer" />
    </declare-styleable>
</resources>